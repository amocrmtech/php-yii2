{
  "name": "amocrmtech/ar-models",
  "description": "AR Models",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Kulikov",
      "email": "r.v.kulikov@yandex.ru"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-pdo": "*",
    "yiisoft/yii2": "~2.0.30",
    "hiqdev/composer-config-plugin": "~0.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "amocrmtech\\models\\ar\\": "src/"
    }
  },
  "extra": {
    "config-plugin": {
      "params": [
        "config/params.php",
        "?config/params-local.php"
      ],
      "common": "config/common.php"
    }
  }
}